{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Xbim": {
    "Server": {
      "BaseUrl": "https://api.Xbim.example.com",
      "RequireAuthentication": true,
      "TimeoutSeconds": 30
    },
    "Standalone": {
      "Theme": {
        "InitialTheme": "Dark",
        "LightAccentColor": "#0969da",
        "DarkAccentColor": "#1e7e34"
      },
      "FileLoaderPanel": {
        "AllowIfcFiles": false,
        "AllowCustomHeaders": false,
        "AutoCloseOnLoad": true
      }
    }
  }
}
